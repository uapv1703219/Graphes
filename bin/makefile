Graphes: ../src/main.cpp Graphe.o GetData.o Pile.o
	g++ -o Graphes.exe ../src/main.cpp Graphe.o GetData.o Pile.o -std=c++11

Graphe.o: ../src/Graphe.cpp ../src/Graphe.h
	g++ -c ../src/Graphe.cpp -std=c++11

GetData.o: ../src/GetData.cpp ../src/GetData.h
	g++ -c ../src/GetData.cpp -std=c++11

Pile.o: ../src/Pile.cpp ../src/Pile.h
	g++ -c ../src/Pile.cpp -std=c++11